ules":"30"},{"filePath":"52","messages":"53","errorCount":0,"warningCount":0,"fixableErrorCount":0,"fixableWarningCount":0,"usedDeprecatedRules":"30"},{"filePath":"54","messages":"55","errorCount":0,"warningCount":1,"fixableErrorCount":0,"fixableWarningCount":0,"source":"56","usedDeprecatedRules":"30"},{"filePath":"57","messages":"58","errorCount":0,"warningCount":0,"fixableErrorCount":0,"fixableWarningCount":0},"/Users/kingnordee/Documents/Online Classes/1SummitJava2020/Assignments/HotelFullStack/hotel_client/src/index.js",[],["59","60"],"/Users/kingnordee/Documents/Online Classes/1SummitJava2020/Assignments/HotelFullStack/hotel_client/src/App.js",[],"/Users/kingnordee/Documents/Online Classes/1SummitJava2020/Assignments/HotelFullStack/hotel_client/src/reportWebVitals.js",[],"/Users/kingnordee/Documents/Online Classes/1SummitJava2020/Assignments/HotelFullStack/hotel_client/src/components/Home.jsx",["61"],"import { useEffect, useState } from \"react\";\nimport axios from \"axios\";\nimport HotelCard from \"./HotelCard\";\nimport { API_URL } from \"../Constants\";\n\nconst Home = () => {\n\n    const [state, setState] = useState({\n        hotels: [], loaded: false, error: false, errorMsg: \"\"\n    })\n\n    const style = {\n        display: \"flex\", justifyContent: \"center\", flexWrap: \"wrap\"\n    }\n\n    useEffect(() => {\n        axios.get(API_URL).then( response => {\n            setState({\n                ...state, loaded: true, error: false, hotels: [...response.data]\n            });\n        }).catch(error => {\n            setState({ ...state, error: true, errorMsg: error })\n            console.log(error);\n        }).then(() => {\n            //Always runs\n        });\n    }, [])\n\n    if(state.error) return <div><br/> Error loading hotels: {state.errorMsg.message}! </div>\n    else if(!state.loaded) return <div><i> Loading hotels... </i></div>\n    else {//else starts\n        return (\n            <div style={style}>\n                {\n                    state.hotels.map(hotel => {\n                           return <HotelCard key={hotel.id} hotel={hotel}/>\n                    })\n                }\n                { state.hotels.length < 1 && <div style={{color:\"red\"}}><br/>Sorry, no hotels are available at the moment!</div>}\n            </div>\n        )\n    }//else ends\n}\n\nexport default Home;\n","/Users/kingnordee/Documents/Online Classes/1SummitJava2020/Assignments/HotelFullStack/hotel_client/src/components/NavBar.jsx",[],"/Users/kingnordee/Documents/Online Classes/1SummitJava2020/Assignments/HotelFullStack/hotel_client/src/components/HotelCard.jsx",[],"/Users/kingnordee/Documents/Online Classes/1SummitJava2020/Assignments/HotelFullStack/hotel_client/src/components/Rooms.jsx",["62"],"import { useEffect, useState } from \"react\";\nimport {useSelector, useDispatch} from \"react-redux\";\nimport { NavLink } from \"react-router-dom\";\nimport axios from \"axios\"\nimport RoomCard from \"./RoomCard\";\nimport AddRoomForm from \"./AddRoomForm\";\nimport { API_URL, SHOW_ADD } from \"../Constants\";\n\nconst Rooms = ( props ) => {\n    const [state, setState] = useState({\n        rooms: [], loaded: false, error: false,\n        hotelId: props.match.params.hotelId, hotelName: null,\n        add: false\n    })\n\n    const select = useSelector(cur => cur.hotelReducer)\n    const dispatch = useDispatch()\n\n    useEffect(() => {\n        axios.get(`${API_URL}/findRooms/${state.hotelId}`)\n            .then( response => {\n            setState({\n                ...state, loaded: true, error: false, rooms: [...response.data],\n                hotelName: JSON.parse(localStorage.getItem(\"hotel\")).name,\n            });\n            // dispatch({type: SHOW_ADD, payload: false})\n        }).catch(error => {\n            setState({ ...state, error: true })\n            console.log(error);\n        }).then(() => {\n            //Always runs\n        });\n    }, [select.showAdd])\n\n    const buttons = {\n        border: \"none\", padding: \"10px\", borderRadius: \"5px\",\n        background: \"none\", color: \"royalblue\",  margin: \"5px\",\n        borderBottom: \"solid royalblue 2px\", fontWeight: \"bolder\"\n    }\n\n    if(state.error) return <div>Error loading rooms!</div>\n    else if(!state.loaded) return <div>Loading rooms...</div>\n    else {\n        return (\n            <div style={{padding:\"0 10vw\"}}>\n                <br/>\n                <div>\n                    <div style={{ display: \"flex\", justifyContent: \"space-between\" }}>\n                        <button style={buttons} onClick={()=>dispatch({type: SHOW_ADD, payload: true})}>Add Room</button>\n                        <h1 style={{margin: 0}}>{state.hotelName}</h1>\n                        <NavLink to=\"/reservations\">\n                            <button style={buttons}>Reservations</button>\n                        </NavLink>\n                    </div>\n                    { select.showAdd && <AddRoomForm/> }\n                </div>\n                {\n                    state.rooms.length > 0 ?\n                        state.rooms.map(room => {\n                            return <RoomCard key={room.roomNum} room={room}/>\n                        })\n                        :\n                        <div style={{color:\"red\"}}><br/> Sorry, there are no rooms available at this moment! </div>\n                }\n            </div>\n        )\n    }\n}\n\nexport default Rooms\n","/Users/kingnordee/Documents/Online Classes/1SummitJava2020/Assignments/HotelFullStack/hotel_client/src/components/RoomCard.jsx",[],"/Users/kingnordee/Documents/Online Classes/1SummitJava2020/Assignments/HotelFullStack/hotel_client/src/Constants.js",["63"],"export const DEV_URL = 'http://localhost:8080'\nexport const PROD_URL = 'http://kingshotel.us-east-2.elasticbeanstalk.com'\nexport const API_URL = PROD_URL\nconst aws_s3_policy = {\n    \"Version\": \"2012-10-17\",\n    \"Statement\":[\n        {\n            \"Sid\": \"AddPerm\",\n            \"Effect\": \"Allow\",\n            \"Principal\": \"*\",\n            \"Action\": \"s3:GetObject\",\n            \"Resource\": \"arn:aws:s3:::kingzhotels/*\"\n        }\n    ]\n}\n\nexport const SET_HOTEL = \"SET_HOTEL\"\nexport const SHOW_ADD = \"SHOW_ADD\"\n","/Users/kingnordee/Documents/Online Classes/1SummitJava2020/Assignments/HotelFullStack/hotel_client/src/reducers/rootReducer.js",[],"/Users/kingnordee/Documents/Online Classes/1SummitJava2020/Assignments/HotelFullStack/hotel_client/src/components/AddRoomForm.js",[],"/Users/kingnordee/Documents/Online Classes/1SummitJava2020/Assignments/HotelFullStack/hotel_client/src/components/Reservations.js",["64"],"import { useEffect, useState } from \"react\";\nimport {NavLink} from \"react-router-dom\";\nimport axios from \"axios\";\nimport {API_URL} from \"../Constants\";\nimport \"../styles/reservations.css\"\nimport ResCard from \"./ResCard\";\n\nconst Reservations = () => {\n\n    const [state, setState] = useState({\n        loaded: false, error: false, reservations: []\n    })\n\n    useEffect(() => {\n        axios.get(`${API_URL}/findReservations`).then(response => {\n            setState({...state, reservations: response.data, loaded: true})\n        }).catch(e => {\n            console.log(`${e}`);\n            setState({...state, error: true})\n        })\n    }, [])\n\n    if(state.error) return <div><br/> Error loading reservations</div>\n    else if(!state.loaded) return <div><i> Loading reservations... </i></div>\n    else {//else starts\n        return <div className=\"resWrap\">\n            <table>\n                <thead>\n                    <tr>\n                        <th>Room <br/> Number</th>\n                        <th>Occupant</th>\n                        <th>Hotel</th>\n                        <th>Rate</th>\n                        <th>User <br/> Action</th>\n                    </tr>\n                </thead>\n                <tbody>\n                    {state.reservations.map(res => {\n                        return <ResCard key={res.roomNum} res={res}/>\n                    })}\n                    {state.reservations < 1 && <tr><td colSpan=\"5\" style={{color:\"red\", border:\"none\"}}>\n                            No reservations found!\n                    </td></tr>}\n                </tbody>\n            </table>\n\n            <br/>\n            <NavLink to={\n                `/${JSON.parse(localStorage.getItem(\"hotel\")).id}/rooms`\n            }>return to rooms</NavLink>\n        </div>\n\n    }\n}\n\nexport default Reservations\n\n","/Users/kingnordee/Documents/Online Classes/1SummitJava2020/Assignments/HotelFullStack/hotel_client/src/components/ResCard.js",[],{"ruleId":"65","replacedBy":"66"},{"ruleId":"67","replacedBy":"68"},{"ruleId":"69","severity":1,"message":"70","line":27,"column":8,"nodeType":"71","endLine":27,"endColumn":10,"suggestions":"72"},{"ruleId":"69","severity":1,"message":"70","line":33,"column":8,"nodeType":"71","endLine":33,"endColumn":24,"suggestions":"73"},{"ruleId":"74","severity":1,"message":"75","line":4,"column":7,"nodeType":"76","messageId":"77","endLine":4,"endColumn":20},{"ruleId":"69","severity":1,"message":"70","line":21,"column":8,"nodeType":"71","endLine":21,"endColumn":10,"suggestions":"78"},"no-native-reassign",["79"],"no-negated-in-lhs",["80"],"react-hooks/exhaustive-deps","React Hook useEffect has a missing dependency: 'state'. Either include it or remove the dependency array. You can also do a functional update 'setState(s => ...)' if you only need 'state' in the 'setState' call.","ArrayExpression",["81"],["82"],"no-unused-vars","'aws_s3_policy' is assigned a value but never used.","Identifier","unusedVar",["83"],"no-global-assign","no-unsafe-negation",{"desc":"84","fix":"85"},{"desc":"86","fix":"87"},{"desc":"84","fix":"88"},"Update the dependencies array to be: [state]",{"range":"89","text":"90"},"Update the dependencies array to be: [select.showAdd, state]",